#!/usr/bin/bash

CLEAN="$(
< raw \
	field 1 2 3 4 5 |
	sed -e 's/://' \
	    -e 's/ / schedattempts /' \
	    -e 's/user/ user/' \
	    -e 's/system/ system/' \
	    -e 's/elapsed/ elapsed/' \
	    -e 's/%CPU/ %CPU/')"

USER_AND_SYSTEM="$( \
	( \
		echo scale=2 ; echo "$CLEAN" | \
			sed -e 's/^.* \([0-9]*\.[0-9]*\) user \([0-9]*\.[0-9]*\) system .*$/\1 + \2/' \
	) | bc | sed 's/$/ user+system/')"

paste <(echo "$CLEAN") <(echo "$USER_AND_SYSTEM") |
	field 1 2 3 4 5 6 11 12 7 8 9 10 \
		> clean
