#!/usr/bin/bash

while read start end ; do
	i=$start
	while [ $i -lt $end ] ; do
		echo $i: $(/bin/time ./test-mp-O3-no-LWT_DEBUG-x64 123456 $i 2>&1)
		echo $i: $(/bin/time ./test-mp-O3-no-LWT_DEBUG-x64 123456 $i 2>&1)
		echo $i: $(/bin/time ./test-mp-O3-no-LWT_DEBUG-x64 123456 $i 2>&1)
		echo $i: $(/bin/time ./test-mp-O3-no-LWT_DEBUG-x64 123456 $i 2>&1)
		i=$[ i + 10 ]
	done
done <<LIST
0 450
LIST

# 0 60
# 150 400

echo pthreads: $(/bin/time ./test-pthread-O3-g-x64 123456 2>&1)
echo pthreads: $(/bin/time ./test-pthread-O3-g-x64 123456 2>&1)
echo pthreads: $(/bin/time ./test-pthread-O3-g-x64 123456 2>&1)
echo pthreads: $(/bin/time ./test-pthread-O3-g-x64 123456 2>&1)

