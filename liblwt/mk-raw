#!/usr/bin/bash

# STEPS=1000000
# COUNT=10

STEPS=123456
COUNT=2

PROG=./gen/test-mp-BITS-O3-g-x64
# PROG=./gen/test-mp-smt-BITS-O3-g-x64

while read START END ; do
	ATTEMPTS=$START
	while [ $ATTEMPTS -le $END ] ; do
		C=0
		while [ $C -lt $COUNT ] ; do
			echo $ATTEMPTS: $(/bin/time $PROG $STEPS $ATTEMPTS 2>&1)
			C=$[ C + 1 ]
		done
		ATTEMPTS=$[ ATTEMPTS + 10 ]
	done
done <<LIST
0 1000
LIST

C=0
while [ $C -lt $COUNT ] ; do
	echo pthreads: $(/bin/time ./gen/test-pthread-O3-g-x64 $STEPS 2>&1)
	C=$[ C + 1 ]
done
